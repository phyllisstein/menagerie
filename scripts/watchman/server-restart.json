[
  "trigger",
  "/app",
  {
    "name": "server-restart",
    "expression": [
      "name",
      [
        "next.config.mjs",
        "src/pages/_app.tsx",
        "src/pages/_document.tsx",
        "tsconfig.json",
        "app.cjs"
      ],
      "wholename"
    ],
    "command": [
      "/app/scripts/develop.sh",
      "serve"
    ],
    "append_files": false
  }
]
