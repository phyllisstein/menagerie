[
  "trigger",
  "/app",
  {
    "name": "server-restart",
    "expression": [
      "name",
      [
        "next.config.js",
        "tsconfig.json",
        "src/_app.tsx",
        "src/_document.tsx"
      ],
      "wholename"
    ],
    "command": [
      "./scripts/develop.sh",
      "serve"
    ],
    "append_files": false
  }
]
